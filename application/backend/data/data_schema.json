        {
            "physical_name": "avg_days_to_resolve",
            "display_name": "Average Days to Resolve",
            "expression": "round(avg(time_to_resolve_sec/60/60/24), 1)",
            "data_type": "float",
            "description": "Average resolution time in days.",
            "description_to_user": "(Average) Shows how many days it typically takes to resolve these types of requests, from submission to completion.",
            "synonym": [
                "resolution time",
                "average time",
                "mean days",
                "typical days",
                "time to fix",
                "days to complete",
                "resolution duration"
            ],
            "data_source_id": 1
        },
        {
            "physical_name": "median_days_to_resolve",
            "display_name": "Median Days to Resolve",
            "expression": "round(median(time_to_resolve_sec/60/60/24), 1)",
            "data_type": "float",
            "description": "Median resolution time in days.",
            "description_to_user": "(Median) Shows how many days it typically takes to resolve these types of requests, from submission to completion.",
            "synonym": [
                "resolution time median",
                "median time",
                "median days"
            ],
            "data_source_id": 1
        },
        {
            "physical_name": "population",
            "display_name": "Population (2020)",
            "expression": "list_sum(list_transform(list(distinct json_array(neighborhood_code, population_2020)), x -> cast(x[1] as integer)))",
            "data_type": "integer",
            "description": "Population (neighborhood level) based on 2020 census data.",
            "description_to_user": "The total number of people living in the area. This is based on the 2020 census data.",
            "synonym": [
                "number of people",
                "total population",
                "population count",
                "people count",
                "residents",
                "local population"
            ],
            "data_source_id": 3
        },
        {
            "physical_name": "requests_per_day",
            "display_name": "# of Requests per Day",
            "expression": "round(count(1) / count(distinct created_date), 2)",
            "data_type": "float",
            "description": "Average number of requests per day.",
            "description_to_user": "The average number of service requests submitted each day.",
            "synonym": [
                "daily requests",
                "requests per day",
                "daily average",
                "average daily requests",
                "requests per day average"
            ],
            "data_source_id": 1
        },
        {
            "physical_name": "requests_per_capita",
            "display_name": "Requests per Capita",
            "expression": "round(count(1) / list_sum(list_transform(list(distinct json_array(neighborhood_code, population_2020)), x -> cast(x[1] as integer))), 2)",
            "data_type": "float",
            "description": "Requests per one person in the area.",
            "description_to_user": "The average number of service requests per person in the area.",
            "synonym": [
                "requests per capita",
                "normalized requests",
                "population adjusted requests",
                "request rate per capita"
            ],
            "data_source_id": 1
        },
        {
            "physical_name": "pct_open_requests",
            "display_name": "% Open Requests",
            "expression": "round(sum(if(status = 'Open', 1, 0)) * 100.0 / count(1), 1)",
            "data_type": "float",
            "description": "Percentage of service requests currently in 'Open' status.",
            "description_to_user": "The percentage of requests that are still active and haven't been closed.",
            "synonym": [
                "open rate",
                "percent open",
                "backlog percentage",
                "unresolved percentage"
            ],
            "data_source_id": 1
        },
        {
            "physical_name": "pct_closed_requests",
