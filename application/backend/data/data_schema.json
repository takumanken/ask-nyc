    "measures": [
        {
            "physical_name": "num_of_requests",
            "display_name": "# of Requests",
            "expression": "count(1)",
            "data_type": "integer",
            "description": "Number of service requests.",
            "description_to_user": "Counts how many service requests match your current selection or filters.",
            "synonym": [
                "count",
                "total",
